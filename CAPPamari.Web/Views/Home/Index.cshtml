<div data-bind="foreach: requirementSets">
    <div class="requirementBox">
        <h3><a href="#" data-bind="text: name, attr: { 'data-reqSetName': name }"></a></h3>
        <div class="courses" data-bind="foreach: appliedCourses">
            <div class="course">
                <!-- when we decide what the course boxes are going to look like we will add that population here -->
                <span data-bind="text: department"></span>
                <span>-</span>
                <span data-bind="text: number"></span>
                <span>(</span>
                <span data-bind="text: semester"></span>
                <span>)</span>
                <span class="fa fa-trash-o" title="Delete this course" data-bind="click: DeleteCourse"></span><br />
                <div class="courseData" data-bind="attr: { 'data-course': JSON.stringify(ToJSONCourse($data)) }" ></div>
            </div>
        </div>
    </div>
</div>